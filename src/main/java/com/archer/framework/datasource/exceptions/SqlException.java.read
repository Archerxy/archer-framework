package com.archer.framework.datasource.exceptions;

public class SqlException extends Exception {

    static final long serialVersionUID = -312146793124229948L;
    
    static final int END_OFFSET = 32;
    
    public SqlException(Throwable e) {
    	super(e);
    }
    
    public SqlException(String msg) {
    	super(msg);
    }
    
    public SqlException(String msg, Throwable e) {
    	super(msg, e);
    }
}
